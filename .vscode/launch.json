{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Firefox - Launch",
      "type": "firefox",
      "request": "launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}",
      "reAttach": true,
      "pathMappings": [
        {
          "url": "webpack://_N_E/",
          "path": "${workspaceFolder}/"
        }
      ],
      "log": {
        "consoleLevel": {
          // "default": "Debug"
        }
      },
      "skipFiles": [
        "${workspaceFolder}/node_modules/**/*.js",
        "${workspaceFolder}/.next/**/*.js"
      ]
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Chrome - Launch",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}"
    }
  ]
}
